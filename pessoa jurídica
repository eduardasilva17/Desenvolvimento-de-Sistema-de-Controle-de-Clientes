namespace Atividade |
{
 Pessoa_Juridica de classe : Clientes
    {
        public string cnpj {get; set;}
        public string ie {get; set;}

 Pagar_Imposto nula de substituição pública (float v)
        {
            this.valor = v;
 this.valor_imposto = este.valor * 20 / 100;
            this.total = this.valor + this.valor_imposto;
        }
    }
}
